{
  "name": "@ibnlanre/cipher",
  "description": "Library for AES encryption or decryption",
  "version": "2.3.0",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "sideEffects": false,
  "files": [
    "esm",
    "cjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibnlanre/cipher.git"
  },
  "keywords": [
    "encrypt",
    "decrypt",
    "AES",
    "encode",
    "decode",
    "cipher",
    "hash",
    "security"
  ],
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "typings": "tsc --project ./",
    "esm-bundle": "tsup --config tsup.esm.ts",
    "cjs-bundle": "tsup --config tsup.cjs.ts",
    "local-test": "node tests/index.js",
    "build": "yarn esm-bundle && yarn cjs-bundle",
    "package": "yarn publish --access public"
  },
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@types/node": "^20.5.6",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "tsup": "^7.2.0",
    "typescript": "^4.8.4",
    "vitest": "^0.34.3"
  }
}
